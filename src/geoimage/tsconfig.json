{
    "compilerOptions": {
      "jsx": "react",
      "target": "ES2020",
      "module": "ESNext",
      "resolveJsonModule":true,
      "moduleResolution": "bundler",
      "skipLibCheck": true,
      "strict": true,
      "declaration": true,
      "declarationDir": "dist",
      "outDir": "dist",
      "noImplicitAny": false,
      "allowSyntheticDefaultImports": true,
      "esModuleInterop": true,
      "baseUrl": ".",
      "paths": {
        "@geoimage/*": ["./src/*"]
      },
    },
    "include": ["**/*"],
    "exclude": ["node_modules", "dist"]
  }